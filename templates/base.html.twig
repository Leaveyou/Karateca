<!DOCTYPE html>
<html lang="ro">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}

    </head>
    <body>
        <ul>
            <li><a href="{{ path('homepage') }}">Home</a></li>
            <li><a href="{{ path('google_oauth') }}">Login with Google</a></li>
        </ul>

        <ul>
            <li><a href="{{ path('redis_keys') }}">List all keys</a></li>
            <li><a href="{{ path('redis_add_party') }}">Add random party</a></li>
            <li><a href="{{ path('redis_clear_all') }}">Clear Redis</a></li>
        </ul>

        {% block body %}{% endblock %}
    </body>
</html>
